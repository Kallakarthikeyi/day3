#include<stdio.h>

int main(){

	int n,m,a[n],b[m],i,j,c[m+n];	printf("enter the size of 1st array:\n");

	scanf("%d",&n);

	printf("enter the size of 2nd array:\n");

	scanf("%d",&m);

	for(i=0;i<n;i++)

	scanf("%d",&a[i]);

	for(j=0;j<m;j++)

	scanf("%d",&b[j]);

	for(i=n,j=0;i<m+n&&j<m;i++,j++){

		a[i]=b[j];

	}

	n=n+m;

	for(i=0;i<n;i++)c[i]=a[i];

	for(i=0;i<n;i++)printf("%d ",c[i]);

	return 0;

/*	int k;

	for(i=0;i<n;i++)c[i]=a[i];

	for(j=0,k=n;j<m&&k<m+n;j++,k++){

	c[k]=b[j];

}*/

for(i=0;i<m+n;i++)printf("%d ",c[i]);

return 0;

}
