#include<stdio.h>

int main(){

	int n,i,a[n],ele,f=0;	printf("enter the size of array:\n");

	scanf("%d",&n);

	printf("enter elements\n");

	for(i=0;i<n;i++)scanf("%d",&a[i]);

	printf("enter the an element for frequency check\n");

	scanf("%d",&ele);

	for(i=0;i<n;i++){

		if(ele==a[i])f++;

	}

	printf("frequency:%d\n",f);

	return 0;

}
